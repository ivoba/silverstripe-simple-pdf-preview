{
    "name": "ivoba/silverstripe-simple-pdf-preview",
    "description": "Simple Pdf Preview Extension for SilverStripe CMS",
    "keywords": [
        "silverstripe",
        "cms",
        "pdf",
        "pdf preview"
    ],
    "license": "MIT",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Ivo Bathke",
            "email": "ivo.bathke@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-imagick": "*",
        "silverstripe/framework": "^4",
        "silverstripe/asset-admin": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Ivoba\\SilverStripe\\SimplePdfPreview\\": "src/",
            "Ivoba\\SilverStripe\\SimplePdfPreview\\tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
